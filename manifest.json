{
    "name": "Request Specific Extension",
    "description": "Starter code for a chrome extension that is request specific.",
    "version": "1.0.0",
    "manifest_version": 3,
    "permissions": [
        "scripting",
        "activeTab",
        "webRequest"
    ],
    "background": {
        "service_worker": "background.js"
    },
    "host_permissions": [
        "<all_urls>"
    ],
    "action":{
        "default_title": "Request Specific Extension",
        "default_icon": "assets/icon.png"
    }
}